@Article{Azimov1985,
  author="Azimov, Ya. I.
    and Dokshitzer, Y. L.
    and Khoze, V. A.
    and Trovan, S. I.",
  title="Similarity of parton and hadron spectra in QCD jets",
  journal="Zeitschrift f{\"u}r Physik C Particles and Fields",
  year="1985",
  month="Mar",
  day="01",
  volume="27",
  number="1",
  pages="65--72",
  abstract="A possible existence of the local correspondence between parton and hadron distributions in hard processes (local parton-hadron duality) is discussed. First comparison is made to check up local duality at modern energies. It is shown, in particular, that an account of essential single-logarithmic corrections to leading double-logarithmic approximation proves sufficient to fit $\pi${\textpm} spectra ine+e− annihilation atW≳20GeV by theoretical perturbative spectra of QCD partons. Rapidity spectra versus the normalized rapidity appear to be similar for $\pi$,K, p. Further tests of local duality as well as effects of chromodynamical coherence are suggested.",
  issn="1431-5858",
  doi="10.1007/BF01642482",
  url="https://doi.org/10.1007/BF01642482"
}

